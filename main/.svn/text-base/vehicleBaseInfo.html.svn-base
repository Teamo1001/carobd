<script type="text/javascript">

       function queryBaseInfo(vin){
    		$.post("../../zend_obd/jsonAPI/vehicleBaseInfo.php",
    				  {
    					 vin:vin
    				  },
    				  function(data,status){
    					  var rows=eval(data);
    			
    					  document.getElementById("nickname").innerText = rows[0].nickName;
    					  document.getElementById("vin_v").innerText = rows[0].v_vin;
    					  document.getElementById("licensenumber").innerText = rows[0].licenseNumber;
    					  document.getElementById("totalmilage").innerText = rows[0].TotalMilage;
    					  document.getElementById("brand1").innerText = rows[0].Brand;
    					  document.getElementById("modelname").innerText = rows[0].ModelName;
    					  document.getElementById("type").innerText = rows[0].Type;
    					  document.getElementById("manuautomatic").innerText = rows[0].manuAutomatic;
    					  document.getElementById("fuelconsumptionstandart").innerText = rows[0].fuelConsumptionStandart;
    					  document.getElementById("tanksize").innerText = rows[0].tanksize;
    					  document.getElementById("swept").innerText = rows[0].Swept;
    					  
    				     
    				     
    				  });
	   
       }
		
</script>


